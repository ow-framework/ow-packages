{
  "compilerOptions": {
    "declaration": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "noUnusedLocals": true,
    "lib": [
      "es6",
      "esnext.asynciterable"
    ]
  }
}